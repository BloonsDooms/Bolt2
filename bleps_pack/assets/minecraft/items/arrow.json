{
    "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/arrow"
    },
    "cases": [
      {
        "when": "1",
        "model": {
          "type": "model",
          "model": "item/arrow_red"
        }
      },
      {
        "when": "2",
        "model": {
          "type": "model",
          "model": "item/arrow_blue"
        }
      },
      {
        "when": "3",
        "model": {
          "type": "model",
          "model": "item/tri_arrow"
        }
      }
    ]
  }
  }