    "model": "item/blank_purp"
  },
  "cases": [
    {
      "when": "1",
      "model": {
        "type": "model",
        "model": "item/blank_star"
      }
    },
    {
      "when": "2",
      "model": {
        "type": "model",
        "model": "item/blank_heart"
      }
    },
    {
      "when": "3",
      "model": {
        "type": "model",
        "model": "item/blank_skull"
      }
    },
    {
      "when": "4",
      "model": {
        "type": "model",
        "model": "item/blank_time"
      }
    }
  ]
}
}